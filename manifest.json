{
  "manifest_version": 2,
  "name": "hPass",
  "description": "HMAC-based password derivation tool",
  "version": "1.2",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Open hPass"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
